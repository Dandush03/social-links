{
	"name": "social-links/social-links",
	"type": "library",
	"description": "PHP library to generate share buttons",
	"keywords": ["share", "social", "facebook", "twitter", "opengraph", "twittercards", "buttons", "links"],
	"homepage": "https://github.com/oscarotero/social-links",
	"license": "MIT",
	"authors": [
		{
			"name": "Oscar Otero",
			"email": "oom@oscarotero.com",
			"homepage": "http://oscarotero.com",
			"role": "Developer"
		}
	],
	"support": {
		"email": "oom@oscarotero.com",
		"issues": "https://github.com/oscarotero/social-links/issues"
	},
	"require": {
		"php": ">=5.4.0"
	},
    "require-dev": {
        "phpunit/phpunit": "^4.8|^5.7|^6.5"
    },
	"suggest": {
		"ext-gd": "Use the GD extension to manipulate the images.",
		"ext-imagick": "Use the Inagick extension to manipulate the images."
	},
	"autoload": {
		"psr-4": {
			"SocialLinks\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"SocialLinks\\": "tests"
		}
	},
	"scripts": {
        "test": [
            "phpunit"
        ]
    }
}
